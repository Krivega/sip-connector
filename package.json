{
  "name": "sip-connector",
  "version": "5.4.0",
  "description": "Module for connect to Vinteo server",
  "keywords": [
    "webrtc",
    "Vinteo"
  ],
  "bugs": {
    "url": "https://github.com/Krivega/sip-connector/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Krivega/sip-connector.git"
  },
  "license": "MIT",
  "author": {
    "name": "Krivega Dmitriy",
    "email": "mr.krivega@gmail.com",
    "url": "https://krivega.com"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:ts && yarn build:js",
    "build:js": "NODE_ENV=production rollup --config",
    "build:ts": "tsc --build tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/. --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && CI=true npm test",
    "release": "standard-version",
    "test": "jest"
  },
  "dependencies": {
    "@krivega/cancelable-promise": "^0.3.0",
    "@krivega/jssip": "^3.17.0",
    "events-constructor": "^1.1.4",
    "promise-delay": "^2.1.0",
    "stack-promises": "^0.0.8",
    "webrtc-mock": "^0.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.0",
    "lint-staged": "^12.4.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.6.2",
    "promise-delay": "^2.1.0",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@krivega/jssip": "^3.16.0"
  },
  "main:src": "src/index.ts",
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && CI=true npm test",
      "postchangelog": "prettier --write \"CHANGELOG.md\""
    }
  }
}
