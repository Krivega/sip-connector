{
  "name": "sip-connector",
  "version": "16.1.0",
  "description": "Module for connect to Vinteo server",
  "keywords": [
    "webrtc",
    "Vinteo"
  ],
  "bugs": {
    "url": "https://github.com/Krivega/sip-connector/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Krivega/sip-connector.git"
  },
  "license": "MIT",
  "author": {
    "name": "Krivega Dmitriy",
    "email": "mr.krivega@gmail.com",
    "url": "https://krivega.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./doMock": {
      "types": "./dist/doMock.d.ts",
      "import": "./dist/doMock.js",
      "require": "./dist/doMock.cjs",
      "default": "./dist/doMock.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "yarn lint:ts && yarn lint:js",
    "lint:js": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy=content",
    "lint:ts": "tsc",
    "prepare": "husky",
    "prepublishOnly": "yarn build",
    "preview": "vite preview",
    "release": "standard-version && npm run release:publish && npm publish --access=public",
    "release:alpha": "standard-version --prerelease alpha && npm run release:publish && npm publish --tag alpha",
    "release:major": "standard-version --release-as major && npm run release:publish && npm publish --access=public",
    "release:pre": "standard-version --prerelease && npm run release:publish && npm publish --tag prerelease",
    "release:publish": "git push --follow-tags",
    "test": "jest",
    "test:ci": "cross-env CI=true jest",
    "test:coverage": "yarn test:ci --no-cache --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@krivega/cancelable-promise": "^1.1.4",
    "@krivega/jssip": "^5.2.0",
    "@krivega/timeout-requester": "^1.0.0",
    "debug": "^4.4.1",
    "events-constructor": "^2.0.0",
    "repeated-calls": "^2.4.0",
    "sequent-promises": "^2.0.1",
    "stack-promises": "^2.0.3",
    "ts-debounce": "^4.0.0",
    "ua-parser-js": "^2.0.4",
    "webrtc-mock": "^1.2.1",
    "xstate": "^5.21.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@krivega/eslint-config": "^1.0.2",
    "@types/debug": "^4.1.12",
    "@types/dom-mediacapture-transform": "^0.1.11",
    "@types/jest": "^30.0.0",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "jest-extended": "^6.0.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "@krivega/cancelable-promise": "^1.1.4",
    "@krivega/jssip": "^5.1.3",
    "@krivega/timeout-requester": "^1.0.0",
    "debug": "^4.4.1",
    "events-constructor": "^2.0.0",
    "repeated-calls": "^2.4.0",
    "sequent-promises": "^2.0.1",
    "stack-promises": "^2.0.3",
    "ts-debounce": "^4.0.0",
    "ua-parser-js": "^2.0.4",
    "webrtc-mock": "^1.2.1",
    "xstate": "^5.21.0"
  },
  "packageManager": "yarn@1.22.22"
}
